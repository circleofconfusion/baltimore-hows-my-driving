    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Monthly Summary",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "runtimeExecutable": "node",
            "runtimeArgs": ["-e", "require('./monthly').getPrevMonthStats()"]
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Open Baltimore Monthly",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "runtimeExecutable": "node",
            "runtimeArgs": ["-e", "require('./open-baltimore').getMonthlyRecords(2020, 1)"]
        }
    ]
}